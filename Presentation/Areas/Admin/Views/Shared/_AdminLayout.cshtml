<body>
 

    <div class="left-sidebar-pro">
        <nav id="sidebar" class="">
            <div class="sidebar-header">
                <a href="index.html"><img class="main-logo" src="~/Adminpanel/img/logo/logo.png" alt="" /></a>
                <strong><img src="~/Adminpanel/img/logo/logosn.png" alt="" /></strong>
            </div>
            <div class="nalika-profile">
                <div class="profile-dtl">
                    <a ><img src="~/assets/images/UserAvatar/@Model.Admin.UserAvatar" alt="" /></a>
                    <h2><span class="min-dtn">@Model.Admin.UserName</span></h2>
                </div>
                <div class="profile-social-dtl">
                    <ul class="dtl-social">
                        <li><a href="#"><i class="icon nalika-facebook"></i></a></li>
                        <li><a href="#"><i class="icon nalika-twitter"></i></a></li>
                        <li><a href="#"><i class="icon nalika-linkedin"></i></a></li>
                    </ul>
                </div>
            </div>
            <div class="left-custom-menu-adp-wrap comment-scrollbar">
                <nav class="sidebar-nav left-sidebar-menu-pro">
                    <ul class="metismenu" id="menu1">
                        <li class="">
                            <a class="has-arrow" href="index.html">
                                <i class="icon nalika-home icon-wrap"></i>
                                <span class="mini-click-non">Home</span>
                            </a>
                            <ul class="submenu-angle" aria-expanded="true">
                                <li><a asp-area="Admin" asp-controller="Home" asp-action="Index">Dashboard</a></li>
                                <li><a asp-area="Admin" asp-controller="Home" asp-action="ContactUs">Contact</a></li>
                                <li><a asp-area="Admin" asp-controller="Home" asp-action="AboutUs">About</a></li>

                            </ul>
                        </li>
                        <li>
                            <a class="has-arrow" href="mailbox.html" aria-expanded="false"><i class="icon nalika-diamond icon-wrap"></i> <span class="mini-click-non">Manage Users</span></a>
                            <ul class="submenu-angle" aria-expanded="false">
                                <li><a asp-area="Admin" asp-controller="Users" asp-action="Users"><span class="mini-sub-pro">Users</span></a></li>
                                <li><a asp-area="Admin" asp-controller="Users" asp-action="Admins"><span class="mini-sub-pro">Admins</span></a></li>
                               
                            </ul>
                        </li>
                        <li>
                            <a class="has-arrow" href="mailbox.html" aria-expanded="false"><i class="icon nalika-mail icon-wrap"></i> <span class="mini-click-non">Store</span></a>
                            <ul class="submenu-angle" aria-expanded="false">

                                <li><a asp-area="Admin" asp-controller="Games" asp-action="ListOfGames"><span class="mini-sub-pro">View Games</span></a></li>
                                <li><a asp-area="Admin" asp-controller="Games" asp-action="ListOfPlatforms" ><span class="mini-sub-pro">View Platforms</span></a></li>
                                <li><a asp-area="Admin" asp-controller="Games" asp-action="ListOfGenres"><span class="mini-sub-pro">View Genres</span></a></li>

                             </ul>
                        </li>
                     
                        <li>
                            <a class="has-arrow" href="mailbox.html" aria-expanded="false"><i class="icon nalika-bar-chart icon-wrap"></i> <span class="mini-click-non">Orders</span></a>
                            <ul class="submenu-angle" aria-expanded="false">
                                <li><a title="Bar Charts" asp-area="Admin" asp-action="ListOfOrders" asp-controller="Orders"><span class="mini-sub-pro">All Orders</span></a></li>
                       
                            
                            </ul>
                        </li>
                        <li>
                            <a class="has-arrow" href="mailbox.html" aria-expanded="false"><i class="icon nalika-table icon-wrap"></i> <span class="mini-click-non">Comments</span></a>
                            <ul class="submenu-angle" aria-expanded="false">
                                <li><a title="Peity Charts" asp-area="Admin" asp-action="AllComments" asp-controller="Games"><span class="mini-sub-pro">All Comments</span></a></li>

                            </ul>
                        </li>
                       
                    </ul>
                </nav>
            </div>
        </nav>
