    <!-- Start Welcome area -->
    <div class="all-content-wrapper">



        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="logo-pro">
                        <a href="index.html"><img class="main-logo" src="~/Adminpanel/img/logo/logo.png" alt="" /></a>
                    </div>
                </div>
            </div>
        </div>

        <div class="header-advance-area">

            <div class="header-top-area">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <div class="header-top-wraper">
                                <div class="row">
                                    <div class="col-lg-1 col-md-0 col-sm-1 col-xs-12">
                                        <div class="menu-switcher-pro">
                                            <button type="button" id="sidebarCollapse" class="btn bar-button-pro header-drl-controller-btn btn-info navbar-btn">
                                                <i class="icon nalika-menu-task"></i>
                                            </button>
                                        </div>
                                    </div>
                                  

                                    <div class="col-lg-5 col-md-5 col-sm-12 col-xs-12">
                                        <div class="header-right-info">
                                            <ul class="nav navbar-nav mai-top-nav header-right-menu">
                                             
                                              
                                                <li class="nav-item">
                                                    <a href="#" data-toggle="dropdown" role="button" aria-expanded="false" class="nav-link dropdown-toggle">
                                                        <i class="icon nalika-user nalika-user-rounded header-riht-inf" aria-hidden="true"></i>
                                                        <span class="admin-name">@Model.Admin.UserName</span>
                                                        <i class="icon nalika-down-arrow nalika-angle-dw nalika-icon"></i>
                                                    </a>
                                                    <ul role="menu" class="dropdown-header-top author-log dropdown-menu animated zoomIn">
                                                     
                                                        <li>
                                                            <a  asp-controller="Users" asp-action="UserDetails" asp-route-Id="@Model.Admin.Id"><span class="icon nalika-user author-log-ic"></span> My Profile</a>
                                                        </li>
                                                       
                                                       
                                                        <li>
                                                            <a   asp-controller="Account" asp-action="Logout" ><span class="icon nalika-unlocked author-log-ic"></span> Log Out</a>
                                                        </li>
                                                    </ul>
                                                </li>
                                               
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Mobile Menu start -->
            <div class="mobile-menu-area">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <div class="mobile-menu">
                                <nav id="dropdown">
                                    <ul class="mobile-menu-nav">
                                        <li class="">
                                            <a class="has-arrow" href="index.html">
                                                <i class="icon nalika-home icon-wrap"></i>
                                                <span class="mini-click-non">Ecommerce</span>
                                            </a>
                                            <ul class="submenu-angle" aria-expanded="true">
                                                <li><a asp-area="Admin" asp-controller="Home" asp-action="Index">Dashboard</a></li>
                                                <li><a asp-area="Admin" asp-controller="Home" asp-action="ContactUs">Contact</a></li>
                                                <li><a asp-area="Admin" asp-controller="Home" asp-action="AboutUs">About</a></li>

                                            </ul>
                                        </li>
                                        <li>
                                            <a class="has-arrow" href="mailbox.html" aria-expanded="false"><i class="icon nalika-diamond icon-wrap"></i> <span class="mini-click-non">Manage Users</span></a>
                                            <ul class="submenu-angle" aria-expanded="false">
                                                <li><a asp-area="Admin" asp-controller="Users" asp-action="Users"><span class="mini-sub-pro">Users</span></a></li>
                                                <li><a asp-area="Admin" asp-controller="Users" asp-action="Admins"><span class="mini-sub-pro">Admins</span></a></li>

                                            </ul>
                                        </li>
                                        <li>
                                            <a class="has-arrow" href="mailbox.html" aria-expanded="false"><i class="icon nalika-mail icon-wrap"></i> <span class="mini-click-non">Store</span></a>
                                            <ul class="submenu-angle" aria-expanded="false">
                                                <li><a asp-area="Admin" asp-controller="Games" asp-action="ListOfGames"><span class="mini-sub-pro">View Games</span></a></li>
                                                <li><a asp-area="Admin" asp-controller="Games" asp-action="ListOfPlatforms"><span class="mini-sub-pro">View Platforms</span></a></li>
                                                <li><a asp-area="Admin" asp-controller="Games" asp-action="ListOfGenres"><span class="mini-sub-pro">View Genres</span></a></li>
                                          
                                            </ul>
                                        </li>

                                        <li>
                                            <a class="has-arrow" href="mailbox.html" aria-expanded="false"><i class="icon nalika-bar-chart icon-wrap"></i> <span class="mini-click-non">Orders</span></a>
                                            <ul class="submenu-angle" aria-expanded="false">
                                                <li><a title="Bar Charts" asp-area="Admin" asp-action="ListOfOrders" asp-controller="Orders"><span class="mini-sub-pro">All Orders</span></a></li>

                                            </ul>
                                        </li>
                                        <li>
                                            <a class="has-arrow" href="mailbox.html" aria-expanded="false"><i class="icon nalika-table icon-wrap"></i> <span class="mini-click-non">Comments</span></a>
                                            <ul class="submenu-angle" aria-expanded="false">
                                                <li><a title="Peity Charts" asp-area="Admin" asp-action="AllComments" asp-controller="Games"><span class="mini-sub-pro">All Comments</span></a></li>

                                            </ul>
                                        </li>
                                   
                                    </ul>
                                </nav>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Mobile Menu end -->
            <div class="breadcome-area">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <div class="breadcome-list">
                                <div class="row">
                                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                        <div class="breadcomb-wp">
                                            <div class="breadcomb-icon">
                                                <i class="icon nalika-home"></i>
                                            </div>
                                            <div class="breadcomb-ctn">
                                                <h2>@ViewData["Title"]</h2>
                                                <p>Welcome to  <span class="bread-ntd">@ViewData["Title"]</span></p>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- content  -->
      @RenderBody();

