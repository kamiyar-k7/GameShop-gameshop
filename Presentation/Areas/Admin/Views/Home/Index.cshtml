@model Application.ViewModel.AdminSide.AdminHomeViewModel;

@{
	Layout = "_AdminLayout";

}


<div class="section-admin container-fluid">
    <div class="row admin text-center">
        <div class="col-md-12">
            <div class="row">

                <!-- Users -->
                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12">

                    <div class="admin-content analysis-progrebar-ctn res-mg-t-15">
                        <h4 class="text-left text-uppercase"><b>Users</b></h4>
                        <div class="row vertical-center-box vertical-center-box-tablet">

                            <div class="col-xs-9a cus-gh-hd-pro">
                                <h2 class="text-right no-margin">@Model.counts.UserCount</h2>
                            </div>
                        </div>
                        <div class="progress progress-mini">
                            <div style="width: 100%;" class="progress-bar bg-green"></div>
                        </div>
                    </div>
                </div>
                <!--Users end-->

                <!-- Admins -->
                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12" style="margin-bottom:1px;">
                    <div class="admin-content analysis-progrebar-ctn res-mg-t-30">
                        <h4 class="text-left text-uppercase"><b>Admins</b></h4>
                        <div class="row vertical-center-box vertical-center-box-tablet">
                         
                            <div class="col-xs-9a cus-gh-hd-pro">
                                <h2 class="text-right no-margin">@Model.counts.AdminCount</h2>
                            </div>
                        </div>
                        <div class="progress progress-mini">
                            <div style="width: 100%;" class="progress-bar progress-bar-danger bg-red"></div>
                        </div>
                    </div>
                </div>
                  <!--Admins end-->

                <!-- Orders -->
                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12">
                    <div class="admin-content analysis-progrebar-ctn res-mg-t-30">
                        <h4 class="text-left text-uppercase"><b>Orders</b></h4>
                        <div class="row vertical-center-box vertical-center-box-tablet">
                           
                            <div class="col-xs-9a cus-gh-hd-pro">
                                <h2 class="text-right no-margin">@Model.counts.OrdersCount</h2>
                            </div>
                        </div>
                        <div class="progress progress-mini">
                            <div style="width: 100%;" class="progress-bar bg-blue"></div>
                        </div>
                    </div>
                </div>
                <!-- Orders End -->

                <!-- Games -->
                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12">
                    <div class="admin-content analysis-progrebar-ctn res-mg-t-30">
                        <h4 class="text-left text-uppercase"><b>Games</b></h4>
                        <div class="row vertical-center-box vertical-center-box-tablet">
                           
                            <div class="col-xs-9a cus-gh-hd-pro">
                                <h2 class="text-right no-margin">@Model.counts.GameCount</h2>
                            </div>
                        </div>
                        <div class="progress progress-mini">
                            <div style="width: 100%;" class="progress-bar bg-purple"></div>
                        </div>
                    </div>
                </div>
                <!-- Games End -->

            </div>
        </div>
    </div>
</div>

